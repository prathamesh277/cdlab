#include<stdio.h>
void E();
void T();
void F();
void Eds();
void Tds();
char input[100];
int i,error;

int main()
{
    printf("Recursive descent parsing for grammar\n");
    printf("\nE->TE'\nE'->+TE'/@\nT->FT'\nT'->*FT'/@\nF->(E)/ID\n\n");
    scanf("%s",input);
    
    E();
    
    if(input[i]=='\0' && error==0)
    {
         printf("string is Accepted");
    }
    else{
        printf("String rejected");
    }
 
}
void E()
{
    T();
    Eds();
}
void T()
{
    F();
    Tds();
}
void Eds()
{
    if(input[i]=='+')
    {
        i++;
        T();
        Eds();
    }
}
void Tds()
{
    if(input[i]=='*')
    {
        i++;
        F();
        Tds();
    }
}
void F()
{
    if(input[i]=='(')
    {
        i++;
        E();
        i++;
    }
    else if(input[i]=='i')
    {
        i++;
    }
    else{
        error=1;
    }
}
